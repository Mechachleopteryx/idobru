    <!-- http://purl.obolibrary.org/obo/IDO_0100713 -->

    <owl:Class rdf:about="&obo;IDO_0100713">
        <rdfs:label>Germany</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;IDO_0100711"/>
        <rdfs:comment>Trends in the epidemiology of human brucellosis in Germany show that after a steady decrease in brucellosis incidence from 1962 to the 1980s, a persistent number of cases has been reported in recent years, with the highest incidence in Turkish immigrants.</rdfs:comment>
        <rdfs:seeAlso>PMID: 18258041</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IDO_0100714 -->

    <owl:Class rdf:about="&obo;IDO_0100714">
