    <owl:Class rdf:about="&obo;IDO_0101129">
        <rdfs:label xml:lang="en">Brucella RicA and Rab2 binding</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;GO_0008150"/>
        <rdfs:subClassOf>
            <owl:Restriction>
