    <!-- http://purl.obolibrary.org/obo/IDO_0100170 -->

    <owl:Class rdf:about="&obo;IDO_0100170">
        <rdfs:label>pasteurization</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;OBI_0000011"/>
        <obo:IAO_0000115 xml:lang="en">A planned process of heating a liquid entity, to a specific temperature of a defined length of time span, and then cooling it immediately. It has an objective of prevent the microbial&apos;s growth in the liquid entity, which is a specific input of this planned process.</obo:IAO_0000115>
        <rdfs:comment xml:lang="en">Pasteurization is a process of heating a food, usually liquid, to a specific temperature for a definite length of time, and then cooling it immediately. This process slows microbial growth in food. (http://en.wikipedia.org/wiki/Pasteurization)</rdfs:comment>
        <obo:IAO_0000117 xml:lang="en">YL</obo:IAO_0000117>
    </owl:Class>
    


