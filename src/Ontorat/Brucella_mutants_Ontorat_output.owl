    <!-- http://purl.obolibrary.org/obo/IDO_0110201 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IDO_0110201">
        <rdfs:label>B. melitensis 16M recA mutant</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IDO_0101027"/>
