            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IDO_0100738"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/OGG_3001164513"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
