    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PR_Q8FVH5">
        <rdfs:label>D-erythrulose-1-phosphate dehydrogenase</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PR_000000001"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000053"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/IDO_0100116"/>
