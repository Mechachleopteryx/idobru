                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000053"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/IDO_0100116"/>
            </owl:Restriction>
