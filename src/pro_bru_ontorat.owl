    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PR_Q2YKV9">
        <rdfs:label>Copper/Zinc superoxide dismutase</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PR_000000001"/>
