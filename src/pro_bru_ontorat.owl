    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PR_Q8FY03">
        <rdfs:label>sensor histidine kinase BvrS, putative</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PR_000000001"/>
        <rdfs:subClassOf>
