    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PR_Q9RPX5">
        <rdfs:label>type IV secretion system protein VirB10</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PR_000000001"/>
