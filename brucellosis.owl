        <obo:IAO_0000115>antibiotic that is doxycycline</obo:IAO_0000115>
        <obo:IAO_0000117>YL</obo:IAO_0000117>
        <dc:creator>YL, YH</dc:creator>
        <rdfs:label>doxycycline</rdfs:label>
    </owl:Class>
    


