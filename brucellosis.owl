                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IDO_0100879"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000054"/>
                                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/IDO_0100886"/>
